{
  "name": "vis-three",
  "version": "0.1.3-2",
  "main": "dist/Vis.umd.js",
  "module": "dist/Vis.es.js",
  "types": "types/main.d.ts",
  "license": "MPL-2.0",
  "author": "Shiotsuki_",
  "repository": "https://github.com/Shiotsukikaedesari/vis-three",
  "scripts": {
    "dev": "vite build --watch -c ./utils/build/vite.config.js",
    "build": "tsc && vite build -c ./utils/build/vite.config.js",
    "lint": "npx eslint *.js src/**/*.ts cypress/**/*.js --fix",
    "e2e:open": "cypress open",
    "examples": "vite -c ./utils/build/vite-examples.config.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "website:dev": "vite serve ./website -c ./utils/build/vite-webside.config.js",
    "website:build": "vite build -c ./utils/build/vite-webside.config.js",
    "website:lint": "npx eslint ./website/src/**/*.vue --fix"
  },
  "devDependencies": {
    "@types/three": "^0.133.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "ant-design-vue": "^3.2.6",
    "cypress": "^9.5.4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "less": "^4.1.2",
    "nprogress": "^0.2.0",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "three": "^0.134.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vitepress": "^0.22.4",
    "vue": "^3.2.21",
    "vue-tsc": "^0.37.0",
    "vue2": "npm:vue@^2.6.11"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "@types/keyboardjs": "^2.5.0",
    "@types/uuid": "^8.3.1",
    "keyboardjs": "^2.6.4",
    "uuid": "^8.3.2",
    "three-csg-ts": "^3.1.10"
  },
  "files": [
    "dist",
    "types",
    "examples",
    "package.json",
    "LICENSE"
  ]
