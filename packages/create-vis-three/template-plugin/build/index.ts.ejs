import { Engine, Plugin } from "@vis-three/core";
import { transPkgName } from "@vis-three/utils";
import { name as pkgname } from "./package.json";

export const <%= name.replace(/-/g, '_').toLocaleUpperCase()%>_PLUGIN = transPkgName(pkgname);

export const <%= name.split('-').map(str => str.slice(0, 1).toLocaleUpperCase() + str.slice(1)).join('')%>Plugin: Plugin<Engine> = function () {
  return {
    name: <%= name.replace(/-/g, '_').toLocaleUpperCase()%>_PLUGIN,
    install(engine) {},
    dispose(engine) {},
  };
};
