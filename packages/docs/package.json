  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "rollup-plugin-visualizer": "^5.6.0",
    "vite": "^2.9.15",
    "vitepress": "1.0.0-beta.1",
    "vue": "^3.2.21"
  },
  "dependencies": {
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3"
  }
}
